<?xml version="1.0" encoding="UTF-8"?>
<module>
    <identity>
		<uri>urn:org:ten60:netkernel:mod:db</uri>
		<version>2.1.5</version>
    </identity>
	<info>
		<name>RDBMS Accessors and Utilities</name>
        <description>SQL Services and Utilities</description>
    </info>
    <publisher>
        <name>1060 Research Limited</name>
		<uri>http://www.1060research.com</uri>
    </publisher>
    <licence>
		<name>1060 Public License v1.0</name>
		<uri>http://www.1060research.com/license</uri>
	</licence>
	<export>
		<uri>
			<match>active:SQL(EscapeXML|EncodeElement|DecodeElement)\+.*</match>
			<match>active:sql(Query|BooleanQuery|Update|Batch|(Query|Update)(Blob|Clob)|Proc|TransactionWrapper)\+.*</match>
			<match>ffcpl:/entrypoints.xml</match>
			<match>active:doc-generate\+.*</match>
		</uri>
		<class>
			<match>org\.hsqldb\..*</match>
			<match>org\.ten60\.rdbms\.representation\..*</match>
		</class>
	</export>
	<rewrite>
	</rewrite>
	<mapping>
		<this>
			<match>ffcpl:/(doc/|entrypoints.xml).*</match>
		</this>
		<ura>
			<match>active:sql(Query|BooleanQuery|Update|Batch|(Query|Update)(Blob|Clob)|Proc)\+.*</match>
			<class>org.ten60.rdbms.accessor.RDBMSAccessorImpl2</class>
		</ura>
		<ura>
			<match>active:SQLEscapeXML\+.*</match>
			<class>org.ten60.rdbms.accessor.EscapeXMLToSQLAccessor</class>
		</ura>
		<ura>
			<match>active:SQL(Encode|Decode)Element\+.*</match>
			<class>org.ten60.rdbms.accessor.ElementEncoderDecoderAccessor</class>
		</ura>
		<ura>
			<match>active:sqlTransactionWrapper\+.*</match>
			<class>org.ten60.rdbms.accessor.TransactionWrapperAccessor</class>
		</ura>
		<import>
			<uri>urn:org:ten60:netkernel:ext:xml:core</uri>
			<version-min>1.3.2</version-min>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:ext:layer1</uri>
		</import>
		<import>
			<uri>urn:org:ten60:netkernel:mod:docxter</uri>
		</import>
		<super/>
    </mapping>
	<transreptors>
		<transreptor>org.ten60.rdbms.transreptor.ConfigToRDBMSConnection</transreptor>
	</transreptors>
</module>
