<?xml version="1.0" encoding="UTF-8"?>
<SessionPolicy>
	<zone>
		<match>.*/(admin)/.*</match>
		<path>/</path>
		<token>cookie</token>
		<type>
			<simple/>
		</type>
	</zone>
	<meta>
		<id>1060NetKernelSession</id>
		<max-sessions>100</max-sessions>
		<max-duration>7200000</max-duration>  <!--2 hours-->
		<min-duration>3600000</min-duration>  <!--1 hour-->
	</meta>
</SessionPolicy>
