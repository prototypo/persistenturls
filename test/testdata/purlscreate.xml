<?xml version="1.0" encoding="ISO-8859-1"?>
<purls>
<!-- Type 301.  Moved Permanently to a target URL -->
	<purl id="/testdomain/test301" type="301">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<target url="http://example.com/test301target/"/>
	</purl>
<!-- Type 302.  Simple redirection to a target URL -->
	<purl id="/testdomain/test302" type="302">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<target url="http://example.com/test302target/"/>
	</purl>
<!-- Type 303.  See Also URL -->
	<purl id="/testdomain/test303/" type="303">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<seealso url="http://example.com/see/also/some/more.xml"/>
	</purl>
<!-- Type 307.  Temporary redirect to a target URL -->
	<purl id="/testdomain/test307" type="307">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<target url="http://example.com/test307target/"/>
	</purl>
<!-- Type 404.  Temporarily gone -->
	<purl id="/testdomain/test404" type="404">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
	</purl>
<!-- Type 410.  Permanently gone -->
	<purl id="/testdomain/test410" type="410">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
	</purl>
<!-- Type clone.  Clone an existing PURL -->
	<purl id="/testdomain/testClone" type="clone">
		<basepurl path="/testdomain/test301PURL"/>
	</purl>
<!-- Type chain.  Chain an existing PURL -->
	<purl id="/testdomain/testChain" type="chain">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<basepurl path="/testdomain/test301-1"/>
	</purl>
<!-- Type partial.  Partial-redirect PURL -->
	<purl id="/testdomain/testPartial" type="partial">
		<maintainers>
			<uid>testuser</uid>
		</maintainers>
		<target url="http://example.com/testPartialtarget/"/>
	</purl>
</purls>
